{
  "npmScope": "formax",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "components": {
      "tags": []
    },
    "components-e2e": {
      "tags": [],
      "implicitDependencies": ["components"]
    },
    "components-src-lib-base": {
      "tags": []
    },
    "components-src-lib-button": {
      "tags": []
    },
    "components-src-lib-glass": {
      "tags": []
    },
    "storybook": {
      "tags": []
    },
    "types": {
      "tags": []
    },
    "utils": {
      "tags": []
    },
    "utils-src-lib-style": {
      "tags": []
    },
    "website": {
      "tags": []
    },
    "website-e2e": {
      "tags": [],
      "implicitDependencies": ["website"]
    }
  }
}
